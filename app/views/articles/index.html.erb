<h1>Article</h1>
<ul>
  <% @articles.each do |article| %>
    <li><%= link_to article.title, article_path(article) %></li>
    <%= link_to  "borrar", article_path(article.id), method: :delete, data: { confirm: "estas seguro?" } %>
  <% end %>
</ul>
<p><%= link_to  "Back articles", new_article_path(@article) %></p>